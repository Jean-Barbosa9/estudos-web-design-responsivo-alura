#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/jean/Projetos/estudos-web-design-responsivo-alura/node_modules/.registry.npmjs.org/atob/2.1.2/node_modules/atob/bin/atob.js/node_modules:/home/jean/Projetos/estudos-web-design-responsivo-alura/node_modules/.registry.npmjs.org/atob/2.1.2/node_modules/atob/bin/node_modules:/home/jean/Projetos/estudos-web-design-responsivo-alura/node_modules/.registry.npmjs.org/atob/2.1.2/node_modules/atob/node_modules:/home/jean/Projetos/estudos-web-design-responsivo-alura/node_modules/.registry.npmjs.org/atob/2.1.2/node_modules:/home/jean/Projetos/estudos-web-design-responsivo-alura/node_modules/.registry.npmjs.org/atob/node_modules:/home/jean/Projetos/estudos-web-design-responsivo-alura/node_modules/.registry.npmjs.org/node_modules:/home/jean/Projetos/estudos-web-design-responsivo-alura/node_modules:/home/jean/Projetos/node_modules:/home/jean/node_modules:/home/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../atob/2.1.2/node_modules/atob/bin/atob.js" "$@"
  ret=$?
else 
  NODE_PATH="/home/jean/Projetos/estudos-web-design-responsivo-alura/node_modules/.registry.npmjs.org/atob/2.1.2/node_modules/atob/bin/atob.js/node_modules:/home/jean/Projetos/estudos-web-design-responsivo-alura/node_modules/.registry.npmjs.org/atob/2.1.2/node_modules/atob/bin/node_modules:/home/jean/Projetos/estudos-web-design-responsivo-alura/node_modules/.registry.npmjs.org/atob/2.1.2/node_modules/atob/node_modules:/home/jean/Projetos/estudos-web-design-responsivo-alura/node_modules/.registry.npmjs.org/atob/2.1.2/node_modules:/home/jean/Projetos/estudos-web-design-responsivo-alura/node_modules/.registry.npmjs.org/atob/node_modules:/home/jean/Projetos/estudos-web-design-responsivo-alura/node_modules/.registry.npmjs.org/node_modules:/home/jean/Projetos/estudos-web-design-responsivo-alura/node_modules:/home/jean/Projetos/node_modules:/home/jean/node_modules:/home/node_modules:/node_modules" node  "$basedir/../../../../../../atob/2.1.2/node_modules/atob/bin/atob.js" "$@"
  ret=$?
fi
exit $ret
